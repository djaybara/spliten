{
  "name": "opinion-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:mem": "NODE_OPTIONS='--max-old-space-size=3072' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:migrate": "prisma migrate dev --name init_models",
